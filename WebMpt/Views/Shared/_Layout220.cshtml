<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Script-Type" content="text/javascript" />
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <link rel="shortcut icon" href="http://192.168.100.220/img/logo.png" type="image/x-icon" />


    <!-- Styles&Scripts Render -->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)


    <!-- CSS -->
    <link rel="stylesheet" href="http://192.168.100.220/css/style.css" type="text/css" media="screen, projection, tv" />
    <link rel="stylesheet" href="http://192.168.100.220/css/style-print.css" type="text/css" media="print" />
    <!-- JavaScript -->
    @*
    <script type="text/javascript" src="http://192.168.100.220/js/jquery-1.8.2.min.js"></script>
    *@
    
    <script type="text/javascript" src="http://192.168.100.220/js/jquery.scroller.js"></script>
    <script type="text/javascript">
		$(document).ready(function(){
			$("form").submit(function() {
				document.getElementById('preloader').style.display='block';
				return true;
			});
		});

		$(function () {
		    $(window).scroller({ img_src: 'http://192.168.100.220/img/scrollup.png', height: 0.5 });
        })
    </script>


    <title>@ViewBag.Title /Сектор МПТ</title>
</head>


<body onload="document.getElementById('preloader').style.display = 'none' ">
