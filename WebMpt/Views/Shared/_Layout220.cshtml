        <img src="http://192.168.100.220/img/loading.gif" alt="Loading..." />
    </div>

    <div id="wrapper">
        <!-- Header -->
        <header id="header-wrapper">
            <div id="header">
                <h1>
                    <img src="http://192.168.100.220/img/h1-bg.gif" width="35" height="35" alt="Logo" />
