            </ul>
            <h3>Контакты</h3>
            <p><span>Phone: </span>78-86</p>
            <p><span>E-mail: </span><a href="mailto:mpt@polymir.by">mpt@polymir.by</a></p>
        </div>


        <!-- Content right -->
        <div id="content-box-right">

            @* {xxxBODYxxx} *@
            <div class="content-wrapper">
                <div class="float-left">
                    <p class="site-title">@ViewBag.LeftContent</p>
                </div>
                <div class="float-right">
                    <section id="login">@Html.Partial("_LoginPartial")</section>
                </div>
            </div>
            <div id="body">
                @RenderSection("featured", required: false)
                <section class="content-wrapper main-content clear-fix">
                    @RenderBody()
                </section>
            </div>


            @* {xxxBODYxxx} *@


        </div>
    </div>
    <div id="footer">
        <p><a href="mailto:mpt@polymir.by">Сектор МПТ</a>&copy; 2016г.</p>
    </div>
